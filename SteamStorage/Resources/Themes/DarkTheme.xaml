<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <SolidColorBrush x:Key="Main" Color="#7371FF"/>
    <SolidColorBrush x:Key="MainAdditional" Color="#00AD71"/>
    <SolidColorBrush x:Key="Additional" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="Accent" Color="#0D1117"/>
    <SolidColorBrush x:Key="AccentAdditional" Color="#21262D"/>
    <SolidColorBrush x:Key="PercentPlus" Color="#02B478"/>
    <SolidColorBrush x:Key="PercentMinus" Color="#FD4534"/>

    <Color x:Key="PercentPlusColor" R="2" G="180" B="120" A="255"/>
    <Color x:Key="PercentMinusColor" R="253" G="69" B="52" A="255"/>

</ResourceDictionary>