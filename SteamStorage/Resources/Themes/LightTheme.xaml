<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="Main" Color="#395B64"/>
    <SolidColorBrush x:Key="MainAdditional" Color="#E7F6F2"/>
    <SolidColorBrush x:Key="Additional" Color="#2C3333"/>
    <SolidColorBrush x:Key="Accent" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="AccentAdditional" Color="#A5C9CA"/>
    <SolidColorBrush x:Key="PercentPlus" Color="#02b478"/>
    <SolidColorBrush x:Key="PercentMinus" Color="#fd4534"/>

    <Color x:Key="PercentPlusColor" R="2" G="180" B="120" A="255"/>
    <Color x:Key="PercentMinusColor" R="253" G="69" B="52" A="255"/>

</ResourceDictionary>